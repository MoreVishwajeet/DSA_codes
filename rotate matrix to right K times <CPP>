#include <bits/stdc++.h> 
vector<int> rotateMatRight(vector<vector<int>> mat, int n, int m, int k) {
	k = k%m;

	for(int i=0; i<n; i++){
		for(int j=0; j<m/2; j++){	
			swap(mat[i][j],mat[i][m-1-j]);
		}

		for(int j=0; j<k/2; j++){
			swap(mat[i][j],mat[i][k-1-j]);
		}

		for(int j=0; j<(m-k)/2; j++){
			swap(mat[i][j+k],mat[i][m-1-j]);
		}
	}

	vector <int> result;
	for(int i=0; i<n; i++){
		for(int j=0; j<m; j++){
			result.push_back(mat[i][j]);
		}
	}

	return result;
}
